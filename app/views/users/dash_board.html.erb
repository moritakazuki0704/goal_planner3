<div>
  <%= @user.planner_name %>
</div>
<div>
  <%= @user.secret_word %>
</div>
  <% @keywords.each do |keyword| %>
<ol>
<li>
  <%= keyword.keyword_1 %>
</li>
<li>
  <%= keyword.keyword_2 %>
</li>
<li>
  <%= keyword.keyword_3 %>
</li>
<li>
  <%= keyword.keyword_4 %>
</li>
<li>
  <%= keyword.keyword_5 %>
</li>
<li>
  <%= keyword.keyword_6 %>
</li>
<li>
  <%= keyword.keyword_7 %>
</li>
<li>
  <%= keyword.keyword_8 %>
</li>
<li>
  <%= keyword.keyword_9 %>
</li>
