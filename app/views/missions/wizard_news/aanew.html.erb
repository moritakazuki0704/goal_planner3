<h1>Missions#new</h1>
<p>Find me in app/views/missions/new.html.erb</p>

<!--目標設定作成フォーム-->
<% if params[:goal] %>
  <%= render 'missions/wizard_news/goal', schedule: @schedule %>

<!--目標タスク作成フォーム-->
<% elsif params[:plan] %>
  <%= render 'missions/wizard_news/plan', form: @form %>

<!--ミッションステートメント作成フォーム-->
<% elsif params[:mission] %>
  <%= render 'missions/wizard_news/mission', mission: @mission %>
<% end %>